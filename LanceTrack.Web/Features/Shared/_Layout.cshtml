<!DOCTYPE html>

<html>
<head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <title>LanceTrack</title>

    @Html.Partial(MVC.Shared.Views.Libs)
    @Html.Partial(MVC.Shared.Views.Urls)

    @* TrackTime feature *@
    <script src="@Links.Features.Shared.scripts.dateService_ts" type="text/javascript"></script>

    <script src="@Links.Features.TrackTime.scripts.trackTimeService_ts" type="text/javascript"></script>
    <script src="@Links.Features.TrackTime.scripts.trackTimeController_ts" type="text/javascript"></script>
    <script src="@Links.Features.TrackTime.scripts.module_ts" type="text/javascript"></script>

    <script src="@Links.Features.Shared.scripts.app_ts" type="text/javascript"></script>

    @RenderSection("head", false)
</head>
<body ng-app="lance-track">
    <header class="navbar navbar-static-top navbar-default">
        <div class="container">
            <div class="navbar-header">
                <a class="navbar-brand">
                    LanceTrack
                </a>
            </div>
        </div>
    </header>
    <div class="container">
        @RenderBody()
    </div>
</body>
</html>